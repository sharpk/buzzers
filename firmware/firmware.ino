void blink1() {
	static char state = LED_OFF;
	if (state == LED_ON)
		state = LED_OFF;
	else
		state = LED_ON;
	
	if (first == 1) {
		digitalWrite(BUZZER_LED1, state);
		if (!soundFlag) {
			soundFlag = 1;
			startPlayback(sample, sizeof(sample));
		}
	}
	else {		
		digitalWrite(LED1, state);
	}
}

